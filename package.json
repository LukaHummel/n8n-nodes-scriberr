{
  "name": "n8n-nodes-scriberr",
  "version": "1.0.0",
  "description": "Scriberr nodes for n8n: transcriptions, notes, chat, and summary APIs.",
  "license": "MIT",
  "homepage": "https://github.com/LukaHummel/n8n-nodes-scriberr/",
  "keywords": [
    "n8n-community-node-package"
  ],
  "author": {
    "name": "Luka Hummel",
    "email": "luka@hummel.email"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LukaHummel/n8n-nodes-scriberr.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": [
      "dist/credentials/ScriberrApiKeyApi.credentials.js",
      "dist/credentials/ScriberrJwtApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Scriberr/Scriberr.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "^0.13.0",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
